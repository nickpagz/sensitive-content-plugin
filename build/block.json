{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "nickp/sensitive-content",
  "version": "0.1.0",
  "title": "Sensitive Content",
  "category": "widgets",
  "icon": "hidden",
  "description": "Add a mask and content warning over images to hide sensitive content.",
  "supports": {
    "html": false
  },
  "textdomain": "sensitive-content",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "warningText": {
      "type": "text",
      "default": "Content Warning: This content has been flagged as sensitive, click to view."
    },
    "revealLink": {
      "type": "text",
      "default": "Show Image"
    },
    "textColor": {
      "type": "string",
      "default": "#000000"
    }
  }
}